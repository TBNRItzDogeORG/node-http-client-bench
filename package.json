{
  "name": "node-http-client-bench",
  "version": "1.4.6",
  "description": "Benchmarks for all the popular Node.js HTTP client libraries",
  "main": "index.js",
  "repository": "fdesjardins/node-http-client-bench",
  "author": {
    "name": "Forrest Desjardins",
    "email": "desjardinsfg@gmail.com",
    "url": "github.com/fdesjardins"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "readme": "docker-compose up",
    "start": "docker-compose up"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "benchmark": "^2.1.4",
    "got": "^9.1.0",
    "isomorphic-fetch": "^2.2.1",
    "node-fetch": "^2.2.0",
    "req-fast": "^0.2.17",
    "request": "^2.88.0",
    "superagent": "^3.8.3",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "standard": "^11.0.1"
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "rules": {
      "object-curly-spacing": [
        2,
        "always"
      ],
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "func-style": [
        2,
        "expression"
      ],
      "prefer-arrow-callback": [
        2,
        {
          "allowNamedFunctions": false
        }
      ]
    }
  }
}
